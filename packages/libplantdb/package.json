{
  "private": true,
  "name": "@plantdb/libplantdb",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./output/index.js",
      "types": "./typings/index.d.ts"
    },
    "./*": {
      "import": "./output/*.js",
      "types": "./typings/*.d.ts"
    }
  },
  "module": "./output/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf tsconfig.tsbuildinfo output typings",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "lint": "eslint source && tsc --noEmit",
    "test": "jest"
  },
  "types": "./typings/index.d.ts",
  "dependencies": {
    "csv-parse": "5.2.2",
    "csv-stringify": "6.4.4",
    "luxon": "2.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.9",
    "@types/luxon": "2.4.0",
    "@types/mocha": "10.0.3",
    "@types/source-map-support": "0.5.9",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "source-map-support": "0.5.21",
    "tslib": "2.6.2",
    "typedoc": "0.23.2",
    "typedoc-plugin-markdown": "3.13.2",
    "typescript": "5.2.2"
  }
}
